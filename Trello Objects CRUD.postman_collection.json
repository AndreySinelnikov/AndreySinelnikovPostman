									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Actual value of path variable 'id' is equal to response attribute 'id'\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    var actualId = pm.collectionVariables.get(\"actualId\"); // 'actualId' set by Pre-request Script\r",
									"    pm.expect(jsonData.id).to.eql(actualId);\r",
									"});\r",
									"\r",
